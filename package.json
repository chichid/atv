{
  "name": "atv",
  "private": "true",
  "version": "1.0.0",
  "homepage": "https://github.com/chichid/atv-test#readme",
  "scripts": {
    "postinstall": "lerna bootstrap $NPM_LERNA_SCOPE && lerna exec $NPM_LERNA_SCOPE npm install",
